# Langfuse
LANGFUSE_DB_USER=langfuse
LANGFUSE_DB_PASSWORD=langfuse_password
LANGFUSE_DB_NAME=langfuse
LANGFUSE_SECRET=change-this-in-production-minimum-32-characters
LANGFUSE_PUBLIC_KEY=pk-lf-mock-key
LANGFUSE_SECRET_KEY=sk-lf-mock-key

# Vobio API
MOCK_MODE=true
