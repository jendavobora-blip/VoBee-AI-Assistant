{
  "version": "1.0.0",
  "routing": {
    "strategy": "intelligent",
    "load_balancer": "round_robin",
    "retry_policy": {
      "max_retries": 3,
      "backoff": "exponential",
      "timeout": 30
    }
  },
  "priority": {
    "levels": {
      "critical": 4,
      "high": 3,
      "normal": 2,
      "low": 1
    },
    "task_multipliers": {
      "fraud_detection": 1.5,
      "finance": 1.5,
      "crypto_prediction": 1.2,
      "video_generation": 0.8
    }
  },
  "brain": {
    "decision_making": {
      "enabled": true,
      "confidence_threshold": 0.7,
      "learning_rate": 0.1
    },
    "memory": {
      "short_term_limit": 100,
      "long_term_enabled": true,
      "context_retention": 3600
    },
    "self_improvement": {
      "enabled": true,
      "auto_apply": false,
      "analysis_interval": 3600
    }
  },
  "resources": {
    "default_cpu": "500m",
    "default_memory": "512Mi",
    "max_concurrent_tasks": 100
  }
}
